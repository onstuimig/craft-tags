{
  "name": "onstuimig/craft-tags",
  "description": "A tag manager for Craft CMS",
  "type": "craft-plugin",
  "version": "3.1.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "tags"
  ],
  "support": {
    "docs": "https://github.com/onstuimig/craft-tags/blob/master/README.md",
    "issues": "https://github.com/onstuimig/craft-tags/issues"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Onstuimig",
      "homepage": "https://onstuimig.nl"
    },
    {
      "name": "Ether Creative",
      "homepage": "https://ethercreative.co.uk"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0||^5.0.0",
    "php": "^8.0.2"
  },
	"require-dev": {
      "craftcms/phpstan": "dev-main",
	  	"craftcms/rector": "dev-main"
    },
  "autoload": {
    "psr-4": {
      "onstuimig\\tagmanager\\": "src/"
    }
  },
  "extra": {
    "name": "Tags",
    "handle": "tag-manager",
    "changelogUrl": "https://raw.githubusercontent.com/onstuimig/craft-tags/refs/heads/master/CHANGELOG.md",
    "class": "onstuimig\\tagmanager\\TagManager"
  },
	"scripts": {
    "phpstan": "phpstan --memory-limit=1G"
  },
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}